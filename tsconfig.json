{
  "compilerOptions": {
    "rootDir": "./",
    "baseUrl": ".",
    "module": "ESNext",
    "target": "ESNext",
    "lib": [
      "DOM",
      "ESNext",
    ],
    "strict": true,
    "isolatedModules": false,
    "esModuleInterop": true,
    "jsx": "preserve",
    "skipLibCheck": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "types": [
      "vite/client",
    ],
  },
  "include": [
    "src",
    "cypress",
  ],
  "exclude": [
    "dist",
    "node_modules",
  ],
  "vueCompilerOptions": {
    "target": "auto",
  },
}