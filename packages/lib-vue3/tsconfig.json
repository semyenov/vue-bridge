{
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": "./",
    "jsx": "preserve",
    "jsxFactory": "h",
    "jsxFragmentFactory": "h",
    "jsxImportSource": "vue",
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "Bundler",
    "lib": [
      "DOM",
      "ESNext",
    ],
    "types": [
      "vite/client",
      "./types"
    ],
    "paths": {
      "~/*": [
        "src/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "dist",
  ],
  "vueCompilerOptions": {
    "target": 3.3,
  }
}
