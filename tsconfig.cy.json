{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "lib": [
      "ES2020.Number",
      "ES2020.String",
    ],
    "types": [
      "cypress"
    ],
    "jsx": "preserve",
    "jsxFactory": "h",
    "jsxFragmentFactory": "h"
  },
  "files": [
    "**/*.cy.tsx",
    "**/*.cy.ts",
    "**/*.cy.js",
    "**/*.cy.jsx",
    "cypress"
  ]
}